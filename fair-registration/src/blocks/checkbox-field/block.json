{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "fair-registration/checkbox-field",
	"version": "1.0.0",
	"title": "Checkbox Field",
	"category": "widgets",
	"icon": "yes-alt",
	"description": "Checkbox field for registration forms",
	"keywords": ["checkbox", "field", "input", "registration"],
	"textdomain": "fair-registration",
	"parent": ["fair-registration/form"],
	"supports": {
		"html": false,
		"reusable": false
	},
	"attributes": {
		"label": {
			"type": "string",
			"default": "Checkbox Option"
		},
		"required": {
			"type": "boolean",
			"default": false
		},
		"fieldId": {
			"type": "string",
			"default": ""
		},
		"checked": {
			"type": "boolean",
			"default": false
		},
		"value": {
			"type": "string",
			"default": "1"
		}
	},
	"usesContext": ["fair-registration/formId"],
	"editorScript": "file:./editor.js",
	"editorStyle": "file:./editor.css",
	"render": "file:./render.php"
}
