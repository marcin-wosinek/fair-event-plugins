{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "fair-timetable/timetable",
	"version": "0.1.0",
	"title": "Timetable",
	"category": "widgets",
	"icon": "calendar-alt",
	"description": "A timetable block for displaying scheduled events",
	"example": {},
	"supports": {
		"html": false,
		"anchor": true,
		"align": true
	},
	"attributes": {
		"startTime": {
			"type": "string",
			"default": "09:00"
		},
		"endTime": {
			"type": "string",
			"default": "17:00"
		},
		"hourHeight": {
			"type": "number",
			"default": 4
		}
	},
	"providesContext": {
		"fair-timetable/startTime": "startTime",
		"fair-timetable/endTime": "endTime",
		"fair-timetable/hourHeight": "hourHeight"
	},
	"textdomain": "fair-timetable",
	"editorScript": "file:./editor.js",
	"viewScript": "file:./view.js",
	"editorStyle": "file:./editor.css",
	"style": "file:./view.css",
	"render": "file:./render.php"
}
