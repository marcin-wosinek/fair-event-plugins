{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "fair-timetable/time-slot",
	"version": "1.0.0",
	"title": "Time Slot",
	"category": "widgets",
	"icon": "clock",
	"description": "Display a scheduled event time slot",
	"keywords": ["timetable", "time", "event", "slot"],
	"textdomain": "fair-timetable",
	"supports": {
		"html": false,
		"align": ["wide", "full"],
		"spacing": {
			"padding": true,
			"margin": ["top", "bottom"]
		}
	},
	"attributes": {
		"title": {
			"type": "string",
			"default": ""
		},
		"startHour": {
			"type": "string",
			"default": "09:00"
		},
		"endHour": {
			"type": "string",
			"default": "10:00"
		}
	},
	"usesContext": ["fair-timetable/hourHeight", "fair-timetable/startHour"],
	"script": "file:./index.js",
	"editorScript": "file:./editor.js",
	"style": "file:./style-index.css"
}
