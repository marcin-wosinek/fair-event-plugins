{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "fair-timetable/time-slot",
	"version": "0.1.0",
	"title": "Time Slot",
	"category": "widgets",
	"icon": "clock",
	"description": "A time slot block for timetable events",
	"parent": ["fair-timetable/time-column-body"],
	"example": {},
	"supports": {
		"html": false,
		"anchor": true,
		"color": {
			"background": true,
			"text": true
		}
	},
	"attributes": {
		"startHour": {
			"type": "string",
			"default": "09:00"
		},
		"endHour": {
			"type": "string",
			"default": "10:00"
		},
		"length": {
			"type": "number",
			"default": 1
		}
	},
	"usesContext": [
		"fair-timetable/startHour",
		"fair-timetable/endHour",
		"fair-timetable/length",
		"fair-timetable/hourHeight"
	],
	"textdomain": "fair-timetable",
	"editorScript": "file:./editor.js",
	"viewScript": "file:./view.js",
	"editorStyle": "file:./editor.css",
	"style": "file:./view.css",
	"render": "file:./render.php"
}
