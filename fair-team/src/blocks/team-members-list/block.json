{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "fair-team/team-members-list",
	"version": "1.0.0",
	"title": "Team Members List",
	"category": "widgets",
	"icon": "groups",
	"description": "Display team members associated with this post",
	"keywords": ["team", "members", "staff", "contributors"],
	"textdomain": "fair-team",
	"usesContext": ["postId", "postType"],
	"supports": {
		"html": false,
		"align": ["wide", "full"],
		"spacing": {
			"padding": true,
			"margin": ["top", "bottom"]
		}
	},
	"attributes": {},
	"editorScript": "file:./editor.js",
	"render": "file:./render.php",
	"editorStyle": "file:./editor.css"
}
