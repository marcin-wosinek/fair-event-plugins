{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "fair-schedule/schedule-column",
	"version": "1.0.0",
	"title": "Schedule Column",
	"category": "widgets",
	"icon": "columns",
	"description": "A column container for organizing time blocks by day or place",
	"keywords": ["schedule", "column", "day", "place", "container"],
	"textdomain": "fair-schedule",
	"supports": {
		"html": false,
		"align": ["wide", "full"],
		"spacing": {
			"padding": true,
			"margin": ["top", "bottom"]
		}
	},
	"attributes": {
		"columnTitle": {
			"type": "string",
			"default": ""
		},
		"columnType": {
			"type": "string",
			"default": "day",
			"enum": ["day", "place"]
		}
	},
	"providesContext": {
		"fair-schedule/columnTitle": "columnTitle",
		"fair-schedule/columnType": "columnType"
	},
	"usesContext": [],
	"editorScript": "file:../../../build/blocks/schedule-column/index.js",
	"style": "file:./style.css",
	"editorStyle": "file:./style.css"
}
