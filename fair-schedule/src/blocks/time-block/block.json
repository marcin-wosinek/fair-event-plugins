{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "fair-schedule/time-block",
	"version": "1.0.0",
	"title": "Time Block",
	"category": "widgets",
	"icon": "clock",
	"description": "Display a scheduled event time slot",
	"keywords": ["schedule", "time", "event", "slot"],
	"textdomain": "fair-schedule",
	"supports": {
		"html": false,
		"align": ["wide", "full"],
		"spacing": {
			"padding": true,
			"margin": ["top", "bottom"]
		}
	},
	"attributes": {
		"title": {
			"type": "string",
			"default": ""
		},
		"startHour": {
			"type": "string",
			"default": "09:00"
		},
		"endHour": {
			"type": "string",
			"default": "10:00"
		}
	},
	"editorScript": "file:../../../build/blocks/time-block/index.js",
	"style": "file:./style.css",
	"editorStyle": "file:./style.css"
}
