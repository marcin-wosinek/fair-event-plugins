{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "fair-schedule-blocks/schedule-accordion",
	"version": "0.1.0",
	"title": "Schedule Accordion",
	"category": "widgets",
	"icon": "list-view",
	"description": "An accordion block for displaying schedule items with collapsible sections",
	"example": {},
	"supports": {
		"html": false,
		"anchor": true,
		"align": true
	},
	"attributes": {
		"autoCollapsedAfter": {
			"type": "number",
			"default": 3
		}
	},
	"textdomain": "fair-schedule-blocks",
	"editorScript": "file:./editor.js",
	"viewScript": "file:./view.js",
	"editorStyle": "file:./editor.css",
	"style": "file:./view.css",
	"render": "file:./render.php"
}
